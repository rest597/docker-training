pipeline {
    environment {
        GIT_COMMIT_SHORT = sh(
                script: "printf \$(git rev-parse --short ${GIT_COMMIT})",
                returnStdout: true
        )
    }

    node {
        stage('Initialize'){
            echo "Docker build is starting up...."

            echo "${env.GIT_COMMIT_SHORT}"
            sh "docker ps -a"
        }
    }


}

